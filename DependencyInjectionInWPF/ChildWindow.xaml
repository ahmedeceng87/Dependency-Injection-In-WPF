<Window
    x:Class="DependencyInjectionInWPF.ChildWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Child Window"
    Width="700"
    Height="450">
    <Grid VerticalAlignment="Center">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <TextBlock
            x:Name="tbData"
            Height="25"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            FontSize="13"
            FontWeight="SemiBold" />
        <Button
            Grid.Row="1"
            Width="100"
            Height="25"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            Click="OnGetDataClick"
            Content="GetData" />
    </Grid>
</Window>
